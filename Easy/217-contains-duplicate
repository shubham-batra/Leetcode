// O(n log n) Solution
class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        // Sort array
        sort(nums.begin(),nums.end()); 
        for(int i = 0; i<nums.size()-1; i++) {
            // If current matches next, return false
            if(nums[i]==nums[i+1]) {
                return true;
            }
        }
        // Otherwise, return false
        return false;
    }
};
