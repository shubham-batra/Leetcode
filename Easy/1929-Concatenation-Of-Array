// O(n) solution

class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        // Create an array
        vector<int> answers;
        // Push back twice using a for-loop
        for(int i = 0; i<nums.size(); i++) {
            answers.push_back(nums[i]);
        }
        for(int i = 0; i<nums.size(); i++) {
            answers.push_back(nums[i]);
        }
        return answers;
    }
};
